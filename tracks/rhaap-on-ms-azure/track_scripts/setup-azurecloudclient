#!/bin/bash
az login --username $INSTRUQT_AZURE_SUBSCRIPTION_AAPAZURELAB_USERNAME --password $INSTRUQT_AZURE_SUBSCRIPTION_AAPAZURELAB_PASSWORD > /dev/null
az role assignment create --assignee $INSTRUQT_AZURE_SUBSCRIPTION_AZUREVIEWER_USERNAME --role Reader --scope /subscriptions/$INSTRUQT_AZURE_SUBSCRIPTION_AAPAZURELAB_SUBSCRIPTION_ID
